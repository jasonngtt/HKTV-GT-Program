{
  "endpoint": "/calculate-shipping-fee",
  "method": "POST",
  "description": "Calculate shipping fee for a list of parcels.",
  "request": {
    "parcels": [
      {
        "dimension": {
          "length": 20,
          "width": 15,
          "height": 10
        },
        "weight": 3,
        "temperature_condition": "Ambient",
        "quantity": 2
      },
      {
        "dimension": {
          "length": 25,
          "width": 18,
          "height": 12
        },
        "weight": 7,
        "temperature_condition": "Chill",
        "quantity": 1
      }
      // Add more parcels if needed
    ]
  },
  "response": {
    "total_shipping_fee": 245.00,
    "breakdown": [
      {
        "parcel_index": 0,
        "item_count": 2,
        "volumetric_weight": 1.2,
        "actual_weight": 3,
        "temperature_condition": "Ambient",
        "shipping_fee": 60.00
      },
      {
        "parcel_index": 1,
        "item_count": 1,
        "volumetric_weight": 2.34,
        "actual_weight": 7,
        "temperature_condition": "Chill",
        "shipping_fee": 160.00  // Corrected shipping fee
      }
      // Add breakdown for each parcel
    ]
  }
}
